@model Project_2_Common.Models.MapViewModels.MapUserModel

<div id="chat-buddies" class="chat-buddies">
    @foreach (var buddy in Model.MyChatBuddies)
    {
        <div data-id="@buddy.Id" data-name="@buddy.UserNameStr" class="clearfix text-center chat-with-me log-history-chat-buddies">
            <img src="https://s3-us-west-2.amazonaws.com/s.cdpn.io/195612/chat_avatar_10.jpg" alt="avatar" />
            <div class="about">
                <div class="name">@buddy.UserNameStr</div>
               
            </div>
        </div>
    }
</div>

        <link rel="stylesheet" href="~/css/bootstrap.css" />
        <link rel="stylesheet" href="~/css/site.css" />
        <link href="~/css/animationCSS.css" rel="stylesheet" />
    </environment>
    @RenderSection("Styles", required: false)
</head>

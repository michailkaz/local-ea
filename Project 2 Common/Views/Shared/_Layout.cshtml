    @RenderBody()
    <footer class="footer">
        <p>&copy; 2018 - MVSquare</p>
    </footer>
    <script src="~/js/bootstrap-notify.js"></script>
    <script src="~/lib/signalr.js"></script>
    <script src="~/js/notification.js"></script>
    @RenderSection("Scripts", required: false)

